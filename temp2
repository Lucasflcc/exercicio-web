  <p id="array"></p>

  <script>
  const c1total = [];
  const c1 = document.getElementById("texto");
  
  const tfila = [];
  const t = document.getElementById("texto");
  
  
  function definir1() {
    if(c1.value !== ""){
      c1total.push(t.value);
      document.getElementById("array").innerHTML = tfila;
      t.value = "";
      }
  }
  
  function addfila() {
    if(t.value !== ""){
      tfila.push(t.value);
      document.getElementById("array").innerHTML = tfila;
      t.value = "";
      }
  }
  
  function removefila() {
      tfila.shift();
      document.getElementById("array").innerHTML = tfila;
  }

  </script>

